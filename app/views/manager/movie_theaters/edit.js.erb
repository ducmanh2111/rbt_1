$("#showtimeModal").html("<%= j render 'edit', movie_theater: @movie_theater, abd: @support_movie_theater, theater: @theater %>");
$("#showtimeModalEdit").modal("show");
var theater_id = $("#movie_theater_theater_id option:selected").val();
var rooms = $('#movie_theater_room_id').html();
var options = $(rooms).filter("optgroup[label='<%=@theater.name%>']").html()
if (options != "") {
  $('#movie_theater_room_id').html(options)
  $('#movie_theater_room_id').parent().show()
}
else {
  $('#movie_theater_room_id').empty()
  $('#movie_theater_room_id').parent().hide() 
}
jQuery('#datetimepicker').datetimepicker();
